<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <g:HTMLPanel>
    <g:VerticalPanel spacing="10">
      <g:Label ui:field="confirmLabel">Bạn có chắc muốn xóa liên hệ này không?</g:Label>
      <g:HorizontalPanel spacing="10">
        <g:Button ui:field="yesButton">Xóa</g:Button>
        <g:Button ui:field="noButton">Hủy</g:Button>
      </g:HorizontalPanel>
    </g:VerticalPanel>
  </g:HTMLPanel>
</ui:UiBinder>
